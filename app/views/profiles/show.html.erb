<% title("Användarprofil") %>

<div class="container">
	<div class="row">
		<div class="col-md-3">
			<p>
				<strong>Namn:</strong>
				<%= @profile.name %>
			</p>

			<p>
				<strong>Program:</strong>
				<%= @profile.program %>
			</p>

			<p>
				<strong>Började år:</strong>
				<%= @profile.start_year %>
			</p>
			<%= link_to 'Redigera profil', edit_profile_path(@profile) %>
		</div>		<!-- col-md-6 closes -->
		<div class="col-md-">
			<%= image_tag @profile.avatar.url(:thumb) %>
		</div>		<!-- col-md-6 closes -->
	</div>
	<!-- row closes -->
</div><!-- container closes -->




