        <ul class="loginbar pull-right">
          <li><a href="/"><%= fa_icon("home") %></a></li>
          
          <li class="devider"></li>
          <% if user_signed_in? %>
          <li><%= link_to fa_icon("calendar"),:kalender %></li>
          	
          <li class="devider"></li>
            <li class="dropdown">
              
              <%= link_to fa_icon("user") + (current_user.profile.name || current_user.username), current_user.profile, class: "dropdown-toggle", data: {toggle: "dropdown", delay: "0", :'close-others' => "false"}%>
              <ul class="dropdown-menu pull-right">
                <li><%= link_to "Profil", current_user.profile %></li>                
                <li><%= link_to "Kontoinställningar", :edit_user_registration%></li>                
                <li><%= link_to "Logga ut", :destroy_user_session, method: :delete %></li>                
               </ul>               
            </li>
            
            <% if current_user.role_id != nil && Role.find_by_id(current_user.role_id) != Role.find_by_id(2)%>            	 
            <li class="devider"></li>            
            <li class="dropdown">              
             	<%= link_to current_user.role.title, "#", class: "dropdown-toggle", data: {toggle: "dropdown", delay: "0", :'close-others' => "false"}%>
             	<ul class="dropdown-menu">
              		<% if current_user.admin? %>
              		<li class="dropdown-submenu">
          				<a href="">Admin</a>
              			<ul class="dropdown-menu pull-right"> 
		              	  <li><%= link_to "Roller", :admin_roles %></li>		              	  
		                  <li><%= link_to "Hantera användare", :users%></li>
		                  <li><%= link_to "Hantera kontaktposter",:admin_kontakt%></li>
		                  <li><%= link_to "Hantera bildkategorier",:admin_bildgalleri%></li>		                
               			</ul>
               		</li>
               		<% end %>
              		<% if current_user.moderator?(:event) %>
              		<li class="dropdown-submenu">
          				<a href="">Event</a>
              			<ul class="dropdown-menu pull-right"> 
		              	  <li><%= link_to "Snabbvy av alla event", :events %></li>
		                  <li><%= link_to "Nytt event", :new_event%></li>
               			</ul>
               		</li>
               		<% end %>
               		<% if current_user.moderator?(:nyheter) %>
              		<li class="dropdown-submenu">
          				<a href="">Nyheter</a>
              			<ul class="dropdown-menu pull-right">		              	  
		                  <li><%= link_to "Ny nyhet", :new_news%></li>
               			</ul>
               		</li>
               		<% end %>               		
               		<% if current_user.moderator?(:utskott) %>
              		<li class="dropdown-submenu">
          				<a href="">Utskott</a>
              			<ul class="dropdown-menu pull-right">              			  		              	  
	                  		<% if current_user.moderator?(:styrelse)%>
		                  		<li><%= link_to 'Styrelse',:styrelse %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:fos)%>
		                  		<li><%= link_to 'Föset',:fos %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:cafe)%>
		                  		<li><%= link_to 'Cafémästeriet',:cafe %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:km)%>
		                  		<li><%= link_to 'Kulturministeriet',:km %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:fnu)%>
		                  		<li><%= link_to 'Näringslivsutskottet',:fnu %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:pryl)%>
		                  		<li><%= link_to 'Prylmästeriet',:pryl %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:sexmasteri)%>
		                  		<li><%= link_to 'Sexmästeri',:sexmasteri %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:sanning)%>
		                  		<li><%= link_to 'Sanningsministeriet',:sanning %></li>
	                  		<%end%>
	                  		<% if current_user.moderator?(:studierad)%>
		                  		<li><%= link_to 'Studierådet',:studierad %></li>
	                  		<%end%>
               			</ul>
               		</li>
               		<% end %>
           		</ul>               
            </li>        	
        	<% end %>
            
          <% else %>
            <li>
              <%= link_to fa_icon("sign-in") + ' Logga in', new_user_session_path %>
            </li>
            <li class="devider"></li>
            <li>
              <%= link_to fa_icon("plus") + ' Ny användare', new_user_registration_path %>
            </li>
          <% end %>
        </ul>

